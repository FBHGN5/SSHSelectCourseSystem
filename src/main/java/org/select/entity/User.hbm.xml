<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.select.entity.User" table="user">
        <id name="username" type="java.lang.String">
            <column name="username" length="12" />
            <generator class="assigned" />
        </id>
        <property name="name" column="name" length="20" />
        <property name="password" column="password" length="20" />
        <property name="state" column="state" length="20" type="java.lang.Integer" />
        <property name="createTime" column="create_time" length="20" type="java.util.Date" />
   </class>
    
</hibernate-mapping>