<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.select.entity.Student" table="student">
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>

        <property name="username" column="username" length="255" not-null="true"></property>
        <property name="studentName" column="student_name" length="255" not-null="true"></property>
        <property name="courseName" column="course_name" length="255" not-null="true"></property>
        <property name="courseId" column="course_id" length="12" not-null="true" type="java.lang.Integer"></property>
        <property name="teacherName" column="course_teacher" length="255" not-null="true"></property>
        <property name="classroom" column="classroom" length="255" not-null="true"></property>
        <property name="createTime" column="create_time" type="java.util.Date" length="255"></property>

   </class>
</hibernate-mapping>